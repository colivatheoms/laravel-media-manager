{
    "name": "ivenms/laravel-media-manager-for-v9",
    "description": "A Media Manager Built With Vuejs & Laravel FOR V9",
    "homepage": "https://github.com/ivenms/laravel-media-manager-for-v9",
    "license": "MIT",
    "keywords": [
        "ctf0",
        "media",
        "manager",
        "laravel",
        "gui",
        "MediaManager"
    ],
    "authors": [
        {
            "name": "Muah",
            "email": "muah003@gmail.com"
        },
        {
            "name": "Iven MS",
            "email": "ivenms@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^9.14.1",
        "maennchen/zipstream-php": ">=1.0",
        "ctf0/package-changelog": "*"
    },
    "autoload": {
        "psr-4": {
            "ivenms\\MediaManager\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ivenms\\MediaManager\\MediaManagerServiceProvider"
            ]
        },
        "changeLog": "logs"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-package-install": [
            "@php artisan vendor:publish --provider=\"ivenms\\MediaManager\\MediaManagerServiceProvider\""
        ]
    }
}
